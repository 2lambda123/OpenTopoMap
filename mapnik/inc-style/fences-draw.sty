<Style name="fences-draw">
	<Rule>
		&maxscale_zoom13;
		&minscale_zoom16;
		<Filter>(([barrier] = 'fence') or ([fenced] = 'yes') or ([barrier] = 'wall'))</Filter>
		<LinePatternSymbolizer file="symbols-topo/fence.png"/>
		<LineSymbolizer stroke="black" stroke-width="0.5"/>
	</Rule>
	<Rule>
		&maxscale_zoom14;
		&minscale_zoom16;
		<Filter>([barrier] = 'hedge')</Filter>
		<LinePatternSymbolizer file="symbols-topo/hedge.png"/>
	</Rule>
</Style>
