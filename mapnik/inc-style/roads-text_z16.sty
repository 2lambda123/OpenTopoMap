<Style name="roads-text_z16">
	<Rule>
		&maxscale_zoom16;
		&minscale_zoom16;
		<Filter>[highway] = 'primary' or [highway] = 'secondary' or [highway] = 'tertiary' or [highway] = 'unclassified' or [highway] = 'residential'</Filter>
		<TextSymbolizer face-name="DejaVu Sans Condensed" size="10" fill="black" halo-radius="1" placement="line">[name]</TextSymbolizer>
	</Rule>
</Style>
