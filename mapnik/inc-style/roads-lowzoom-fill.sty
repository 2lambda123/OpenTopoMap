<Style name="roads-lowzoom-fill">

<!-- zoom 6 -->
	<Rule>
		&maxscale_zoom6;
		&minscale_zoom6;
		<Filter>[highway] = 'motorway'</Filter>
		<LineSymbolizer stroke="#fdb548" stroke-width="1.3" />
	</Rule>
	<Rule>
		&maxscale_zoom6;
		&minscale_zoom6;
		<Filter>[highway] = 'trunk'</Filter>
		<LineSymbolizer stroke="#fdfd4f" stroke-width="0.8" />
	</Rule>


<!-- zoom 7 -->
	<Rule>
		&maxscale_zoom7;
		&minscale_zoom7;
		<Filter>[highway] = 'motorway'</Filter>
		<LineSymbolizer stroke="#fdb548" stroke-width="1.6" />
	</Rule>
	<Rule>
		&maxscale_zoom7;
		&minscale_zoom7;
		<Filter>[highway] = 'trunk'</Filter>
		<LineSymbolizer stroke="#fdfd4f" stroke-width="1.6" />
	</Rule>
	<Rule>
		&maxscale_zoom7;
		&minscale_zoom7;
		<Filter>[highway] = 'primary'</Filter>
		<LineSymbolizer stroke="white" stroke-width="1.1" />
	</Rule>


<!-- zoom 8 -->
	<Rule>
		&maxscale_zoom8;
		&minscale_zoom8;
		<Filter>[highway] = 'motorway'</Filter>
		<LineSymbolizer stroke="#fdb548" stroke-width="2" />
	</Rule>
	<Rule>
		&maxscale_zoom8;
		&minscale_zoom8;
		<Filter>[highway] = 'trunk'</Filter>
		<LineSymbolizer stroke="#fdfd4f" stroke-width="1.8" />
	</Rule>
	<Rule>
		&maxscale_zoom8;
		&minscale_zoom8;
		<Filter>[highway] = 'primary'</Filter>
		<LineSymbolizer stroke="white" stroke-width="1.5" />
	</Rule>


<!-- zoom 9 -->
	<Rule>
		&maxscale_zoom9;
		&minscale_zoom9;
		<Filter>[highway] = 'motorway' or [highway] = 'trunk'</Filter>
		<LineSymbolizer stroke="#fdb548" stroke-width="4.2" />
	</Rule>
	<Rule>
		&maxscale_zoom9;
		&minscale_zoom9;
		<Filter>[highway] = 'primary'</Filter>
		<LineSymbolizer stroke="#fdfd4f" stroke-width="3.0" />
	</Rule>
	<Rule>
		&maxscale_zoom9;
		&minscale_zoom9;
		<Filter>[highway] = 'secondary'</Filter>
		<LineSymbolizer stroke="white" stroke-width="1.5" />
	</Rule>

</Style>
