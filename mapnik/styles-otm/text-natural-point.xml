<Style name="text-natural-point">
	<Rule>
		&maxscale_zoom13;
		&minscale_zoom13;
		<Filter>([natural] = 'peak' or [natural] = 'volcano') and ([name] = null) and ([otm_isolation] &gt; 300)</Filter>
		<TextSymbolizer fontset-name="sans-oblique" size="9" fill="black" margin="1" halo-radius="2" halo-fill="rgba(255,255,255,0.8)" placement="point" dy="3" wrap-width="100">[ele]</TextSymbolizer>
	</Rule>
	<Rule>
		&maxscale_zoom13;
		&minscale_zoom13;
		<Filter>([natural] = 'peak' or [natural] = 'volcano') and (not ([name] = null)) and ([otm_isolation] &gt; 300)</Filter>
		<TextSymbolizer fontset-name="sans-oblique" size="9" fill="black" margin="1" halo-radius="2" halo-fill="rgba(255,255,255,0.8)" placement="point" dy="3" wrap-width="100">[name]+"\n"+[ele]</TextSymbolizer>
	</Rule>

	<Rule>
		&maxscale_zoom14;
		&minscale_zoom17;
		<Filter>([natural] = 'peak' or [natural] = 'volcano') and ([name] = null)</Filter>
		<TextSymbolizer fontset-name="sans-oblique" size="9" fill="black" margin="1" halo-radius="2" halo-fill="rgba(255,255,255,0.8)" placement="point" dy="3" wrap-width="100">[ele]</TextSymbolizer>
	</Rule>
	<Rule>
		&maxscale_zoom14;
		&minscale_zoom17;
		<Filter>([natural] = 'peak' or [natural] = 'volcano') and not ([name] = null)</Filter>
		<TextSymbolizer fontset-name="sans-oblique" size="9" fill="black" margin="1" halo-radius="2" halo-fill="rgba(255,255,255,0.8)" placement="point" dy="3" wrap-width="100">[name]+"\n"+[ele]</TextSymbolizer>
	</Rule>

	<Rule>
		&maxscale_zoom14;
		&minscale_zoom17;
		<Filter>([natural] = 'saddle' and [place] = null) and not ([name] = null)</Filter>
		<TextSymbolizer fontset-name="sans-oblique" size="9" fill="black" margin="1" halo-radius="2" halo-fill="rgba(255,255,255,0.8)" placement="point" dy="10" wrap-width="100">[name]+"\n"+[ele]</TextSymbolizer>
	</Rule>
	<Rule>
		&maxscale_zoom14;
		&minscale_zoom17;
		<Filter>([natural] = 'saddle' and [place] = null) and ([name] = null)</Filter>
		<TextSymbolizer fontset-name="sans-oblique" size="9" fill="black" margin="1" halo-radius="2" halo-fill="rgba(255,255,255,0.8)" placement="point" dy="10" wrap-width="100">[ele]</TextSymbolizer>
	</Rule>

	<Rule>
		&maxscale_zoom15;
		&minscale_zoom17;
		<Filter>([natural] = 'col' or [natural] = 'notch') and ([place] = null) and not ([name] = null)</Filter>
		<TextSymbolizer fontset-name="sans-oblique" size="9" fill="black" margin="1" halo-radius="2" halo-fill="rgba(255,255,255,0.8)" placement="point" dy="8" wrap-width="100">[name]+"\n"+[ele]</TextSymbolizer>
	</Rule>
	<Rule>
		&maxscale_zoom15;
		&minscale_zoom17;
		<Filter>([natural] = 'col' or [natural] = 'notch') and ([place] = null) and ([name] = null)</Filter>
		<TextSymbolizer fontset-name="sans-oblique" size="9" fill="black" margin="1" halo-radius="2" halo-fill="rgba(255,255,255,0.8)" placement="point" dy="8" wrap-width="100">[ele]</TextSymbolizer>
	</Rule>

</Style>
