<Style name="landuse-over-water">
	<Rule>
		&maxscale_zoom10;
		&minscale_zoom17;
		<Filter>(([natural] = 'wetland') and ([wetland] = 'swamp'))</Filter>
		<PolygonPatternSymbolizer file="symbols-otm/swamp.png"/>
		<LinePatternSymbolizer file="symbols-otm/swamp-outline.png" opacity="0.7"/>
	</Rule>
	<Rule>
		&maxscale_zoom10;
		&minscale_zoom12;
		<Filter>(([natural] = 'wetland') and ([wetland] = 'bog'))</Filter>
		<PolygonPatternSymbolizer file="symbols-otm/bog.png"/>
		<LineSymbolizer stroke="#0055ff" stroke-width="0.5" stroke-opacity="0.2" />
	</Rule>
	<Rule>
		&maxscale_zoom13;
		&minscale_zoom17;
		<Filter>(([natural] = 'wetland') and ([wetland] = 'bog'))</Filter>
		<PolygonPatternSymbolizer file="symbols-otm/bog.png"/>
		<LineSymbolizer stroke="#0055ff" stroke-width="1" stroke-opacity="0.2" />
	</Rule>
	<Rule>
		&maxscale_zoom10;
		&minscale_zoom17;
		<Filter>(([natural] = 'wetland') and ([wetland] = 'tidalflat'))</Filter>
		<PolygonPatternSymbolizer file="symbols-otm/tidalflat.png" opacity="0.7"/>
	</Rule>
	<Rule>
		&maxscale_zoom11;
		&minscale_zoom17;
		<Filter>(([natural] = 'wetland') and ([wetland] = 'reedbed'))</Filter>
		<PolygonPatternSymbolizer file="symbols-otm/reedbed.png"/>
	</Rule>
	<Rule>
		&maxscale_zoom10;
		&minscale_zoom17;
		<Filter>(([natural] = 'wetland') and not (([wetland] = 'bog') or ([wetland] = 'swamp') or ([wetland] = 'tidalflat') or ([wetland] = 'reedbed')))</Filter>
		<PolygonPatternSymbolizer file="symbols-otm/swamp.png"/>
		<LinePatternSymbolizer file="symbols-otm/swamp-outline.png" opacity="0.7"/>
	</Rule>
	<Rule>
		&maxscale_zoom12;
		&minscale_zoom17;
		<Filter>([natural] = 'beach') or ([natural] = 'sand')</Filter>
		<PolygonPatternSymbolizer file="symbols-otm/sand.png" opacity="0.7"/>
	</Rule>
</Style>
