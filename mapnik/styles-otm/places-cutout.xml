<Style name="places-cutout">
	<Rule>
		&maxscale_zoom9;
		&minscale_zoom17;
		<Filter>[place] = 'city'</Filter>
		<TextSymbolizer face-name="DejaVu Sans Condensed Bold" size="15" fill="black" halo-radius="2" halo-fill="white" placement="point" halo-comp-op="dst-out">[name]</TextSymbolizer>
	</Rule>
</Style>

