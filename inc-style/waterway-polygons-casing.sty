<Style name="waterway-polygons-casing">
<!-- riverbanks -->
	<Rule>
		&maxscale_zoom10;
		&minscale_zoom11;
		<Filter>([waterway] = 'riverbank')</Filter>
		<LineSymbolizer stroke="blue" stroke-width="1.1" />
	</Rule>
	<Rule>
		&maxscale_zoom12;
		&minscale_zoom15;
		<Filter>([waterway] = 'riverbank')</Filter>
		<LineSymbolizer stroke="blue" stroke-width="1.7" />
	</Rule>
</Style>
