<Style name="symbols-viewpoints">
	<Rule>
		&maxscale_zoom14;
		&minscale_zoom15;
		<Filter>([tourism] = 'viewpoint')</Filter>
		<PointSymbolizer file="symbols-topo/viewpoint.png"/>
	</Rule>
	<Rule>
		&maxscale_zoom12;
		&minscale_zoom15;
		<Filter>(([natural] = 'peak') and (not [summit:cross] = 'yes'))</Filter>
		<PointSymbolizer file="symbols-topo/peak.png"/>
	</Rule>
	<Rule>
		&maxscale_zoom12;
		&minscale_zoom15;
		<Filter>(([natural] = 'peak') and ([summit:cross] = 'yes'))</Filter>
		<PointSymbolizer file="symbols-topo/summit_cross.png"/>
	</Rule>
</Style>
